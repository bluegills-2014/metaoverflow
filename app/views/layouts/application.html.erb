<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %></title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <span background-color='red'>^ baller space v</span>
  <header>
    <h1>Metaoverflow</h1>
    <nav>
      <%= link_to 'See latest questions', questions_path %>
      <%= link_to 'Users', users_path  %>
      <% if current_user %>
        <%= link_to("Logout", logout_path)%>
      <% else %>
        <%= link_to_if(current_user.nil?, 'Login', '/login') %>
        <%= link_to_if current_user.nil?, 'New User', new_user_path %>
      <% end %>
    </nav>
  </header>
  <%= yield %>

  </body>
</html>
